{
	"product": {
		"name":	"Brickworks hard clipper example (C++)",
		"version": "1.1.0",
		"buildVersion": "1",
		"bundleName": "bw_example_fxpp_clip",
		"buses": [
			{
				"type":	"audio",
				"direction": "input",
				"channels": "mono",
				"name": "Input",
				"shortName": "Input"
			},
			{
				"type": "audio",
				"direction": "output",
				"channels": "mono",
				"name": "Output",
				"shortName": "Output"
			}
		],
		"parameters": [
			{
				"name": "Gain",
				"shortName": "Gain",
				"direction": "input",
				"defaultValue": 1.0,
				"minimum": 0.1,
				"maximum": 10.0,
				"map": "linear"
			},
			{
				"name": "Bias",
				"shortName": "Bias",
				"direction": "input",
				"defaultValue": 0.0,
				"minimum": -2.5,
				"maximum": 2.5,
				"map": "linear"
			}
		]
	}
}
