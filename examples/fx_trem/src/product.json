{
	"product": {
		"name":	"Brickworks tremolo example",
		"version": "1.1.0",
		"buildVersion": "1",
		"bundleName": "bw_example_fx_trem",
		"buses": [
			{
				"type":	"audio",
				"direction": "input",
				"channels": "mono",
				"name": "Input",
				"shortName": "Input"
			},
			{
				"type": "audio",
				"direction": "output",
				"channels": "mono",
				"name": "Output",
				"shortName": "Output"
			}
		],
		"parameters": [
			{
				"name": "Rate",
				"shortName": "Rate",
				"direction": "input",
				"defaultValue": 5.0,
				"minimum": 1.0,
				"maximum": 20.0,
				"unit": "hz",
				"map": "logarithmic"
			},
			{
				"name": "Amount",
				"shortName": "Amount",
				"direction": "input",
				"defaultValue": 0.0,
				"minimum": 0.0,
				"maximum": 100.0,
				"unit": "pc",
				"map": "linear"
			}
		]
	}
}
